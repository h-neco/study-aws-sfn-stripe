{
  "app": "npx ts-node --prefer-ts-exts bin/sfn-stripe-lab.ts",
  "cli-telemetry": false,
  "watch": {
    "include": ["**/*.ts"],
    "exclude": [
      "README.md",
      "cdk*.json",
      "**/*.d.ts",
      "**/*.js",
      "tsconfig.json",
      "package*.json"
    ]
  },
  "context": {
    "aws-cdk:skip-assets": true,
    "@aws-cdk/core:bootstrapQualifier": "local",
    "aws-cdk:local": true,
    "aws-cdk:overwrite-buckets": {
      "s3": "http://localhost:4566"
    },
    "@aws-cdk/core:skipAssetHashContext": true,
    "aws-cdk:force-path-style": true
  }
}
